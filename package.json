    "lint:markdown": "markdownlint '**/*.md' --dot --ignore node_modules",
    "lint:typescript": "ts-standard",
    "lighthouse": "lhci autorun",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@fontsource/montserrat": "4.2.2",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
