{
  "name": "divlo",
  "version": "0.0.0",
  "private": true,
  "ts-standard": {
    "ignore": [
      ".next",
      ".lighthouseci",
      "node_modules",
      "next-env.d.ts",
      "**/workbox-*.js",
      "**/sw.js"
    ],
    "envs": [
      "node",
      "browser"
    ],
    "report": "stylish"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "export": "next export",
